
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

<form method="post" action="Login">
    <div class="form-group">
        @Html.AntiForgeryToken()
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Login with Google" class="btn btn-default" />
        </div>
    </div>
</form>